  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kylecorbelli/typescript-maybe/issues"
  },
  "homepage": "https://github.com/kylecorbelli/typescript-maybe#readme",
  "dependencies": {
    "ramda": "^0.25.0"
  },
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "@types/ramda": "^0.25.24",
